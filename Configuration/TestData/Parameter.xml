      </ALPHA>
      <BETA>
        <Url>https://discoveryfcu.originsalphasite.net</Url>
      </BETA>
    </DISCOVERY>
    <ENRICHMENT>
      <ALPHA>
        <Url>https://enrichmentfcu.originsalphasite.net</Url>
      </ALPHA>
    </ENRICHMENT>
  </CLIENT>
</PARAMETER>
